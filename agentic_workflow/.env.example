# Google AI Configuration
GOOGLE_API_KEY=your-google-api-key-here

# Supabase Configuration
SUPABASE_CONNECTION_STRING=postgresql://user:password@host:port/database

# Langfuse Configuration for Google ADK
# IMPORTANT: Use these exact variable names for ADK integration
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key
LANGFUSE_SECRET_KEY=your-langfuse-secret-key
LANGFUSE_HOST=https://cloud.langfuse.com

# Enable Langfuse tracing in Google ADK
ADK_LANGFUSE_ENABLED=true

# Optional: Custom trace name
LANGFUSE_TRACE_NAME=re-frame-session

# Optional: Additional metadata
LANGFUSE_METADATA={"app": "re-frame", "version": "0.1.0"}